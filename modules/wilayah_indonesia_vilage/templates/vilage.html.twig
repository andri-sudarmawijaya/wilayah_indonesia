{#
/**
 * @file vilage.html.twig
 * Default theme implementation to present Vilage data.
 *
 * This template is used when viewing Vilage pages.
 *
 *
 * Available variables:
 * - content: A list of content items. Use 'content' to print all content, or
 * - attributes: HTML attributes for the container element.
 *
 * @see template_preprocess_vilage()
 *
 * @ingroup themeable
 */
#}
<div{{ attributes.addClass('vilage') }}>
  {% if content %}
    {{- content -}}
  {% endif %}
</div>
