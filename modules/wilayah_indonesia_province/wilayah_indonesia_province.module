<?php

/**
 * @file
 * Contains wilayah_indonesia_province.module.
 */

use Drupal\Core\Routing\RouteMatchInterface;

/**
 * Implements hook_help().
 */
function wilayah_indonesia_province_help($route_name, RouteMatchInterface $route_match) {
  switch ($route_name) {
    // Main module help for the wilayah_indonesia_province module.
    case 'help.page.wilayah_indonesia_province':
      $output = '';
      $output .= '<h3>' . t('About') . '</h3>';
      $output .= '<p>' . t('Manage Province Data') . '</p>';
      return $output;

    default:
  }
}
