{#
/**
 * @file province.html.twig
 * Default theme implementation to present Province data.
 *
 * This template is used when viewing Province pages.
 *
 *
 * Available variables:
 * - content: A list of content items. Use 'content' to print all content, or
 * - attributes: HTML attributes for the container element.
 *
 * @see template_preprocess_province()
 *
 * @ingroup themeable
 */
#}
<div{{ attributes.addClass('province') }}>
  {% if content %}
    {{- content -}}
  {% endif %}
</div>
